<head>
  <!-- Google tag (gtag.js) -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-GND2K25G3S"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'G-GND2K25G3S');
  </script>
  <meta charset="{{site.charset}}"/>
  <title>
    {% if wp_title %}
      {{ wp_title }} - {{ site.name }}
    {% else %}
      {{ site.name }}
    {% endif %}
  </title>
  <meta name="description" content="{{ site.description }}">
  <link rel="stylesheet" href="{{ site.theme.link }}/style.css" type="text/css" media="screen"/>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="pingback" href="{{ site.pingback_url }}"/>
  {% if options.favicon %}
    <link rel="shortcut icon" href="{{ Image(options.favicon).src|resize(30, 30) }}" />
  {% endif %}
  {{ function('wp_head') }}
</head>
